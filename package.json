{
  "name": "rx-singleton-lock",
  "version": "1.2.0",
  "scripts": {
    "start": "npm run serve",
    "serve": "webpack-serve --config ./example/webpack.config.js --port 8080",
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write index.ts package.json tsconfig*.json lib/**/* test/**/* example/**/*.tsx",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "test": "npm run lint && npm run test:unit && npm run test:integration",
    "test:unit": "mocha --require ts-node/register 'test/unit/*.spec.ts'",
    "test:unit:watch": "npm run test:unit -- --watch --watch-extensions ts",
    "test:integration": "mocha --timeout 60000 --require ts-node/register 'test/integration/*.spec.ts'",
    "test:integration:watch": "npm run test:integration -- --watch --watch-extensions ts"
  },
  "description": "A small util that provides singleton locks for rxjs streams.",
  "license": "MIT",
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "dist/lib"
  ],
  "main": "dist/index",
  "typings": "dist/index.d.ts",
  "keywords": [
    "rxjs",
    "lock",
    "mutex",
    "semaphore"
  ],
  "author": "Christoffer Skeppstedt",
  "repository": {
    "type": "git",
    "url": "https://github.com/cskeppstedt/rx-singleton-lock"
  },
  "homepage": "https://github.com/cskeppstedt/rx-singleton-lock",
  "bugs": "https://github.com/cskeppstedt/rx-singleton-lock",
  "contributors": [
    "Christoffer Skepptsedt (https://chsk.se)"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.2.17",
    "@types/mocha": "^8.2.2",
    "@types/puppeteer": "^5.4.3",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "chai": "^4.3.4",
    "html-webpack-plugin": "^5.3.1",
    "mocha": "^8.3.2",
    "prettier": "^2.2.1",
    "puppeteer": "^9.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rxjs": "^6.3.3",
    "rxjs-tslint": "^0.1.8",
    "ts-loader": "^9.1.1",
    "ts-node": "^9.1.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.4",
    "webpack": "^5.36.1",
    "webpack-cli": "^4.6.0",
    "webpack-serve": "^4.0.0"
  },
  "peerDependencies": {
    "rxjs": "^7.0.0"
  }
}
