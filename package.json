{
  "name": "rx-singleton-lock",
  "version": "1.0.0",
  "scripts": {
    "start": "webpack-dev-server",
    "serve":
      "webpack-serve --config ./example/webpack.config.js --port 8080 --log-level warn --no-clipboard",
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "format":
      "prettier --write index.ts package.json webpack.config.js lib/**/* test/**/*",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "test": "npm run lint && npm run test:unit && npm run test:integration",
    "test:unit": "mocha --require ts-node/register 'test/unit/*.spec.ts'",
    "test:unit:watch": "npm run test:unit -- --watch --watch-extensions ts",
    "test:integration":
      "mocha --timeout 60000 --require ts-node/register 'test/integration/*.spec.ts'",
    "test:integration:watch":
      "npm run test:integration -- --watch --watch-extensions ts"
  },
  "description": "A small util that provides singleton locks for rxjs streams.",
  "license": "MIT",
  "files": ["dist/index.js", "dist/index.d.ts", "dist/lib"],
  "main": "dist/index",
  "typings": "dist/index.d.ts",
  "keywords": ["rxjs", "lock", "mutex", "semaphore"],
  "author": "Christoffer Skeppstedt",
  "repository": {
    "type": "git",
    "url": "https://github.com/cskeppstedt/rx-singleton-lock"
  },
  "homepage": "https://github.com/cskeppstedt/rx-singleton-lock",
  "bugs": "https://github.com/cskeppstedt/rx-singleton-lock",
  "contributors": ["Christoffer Skepptsedt (https://chsk.se)"],
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.0",
    "@types/puppeteer": "^1.2.3",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "chai": "^4.1.2",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.1.1",
    "prettier": "^1.12.1",
    "puppeteer": "^1.3.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "rxjs": "^6.0.0",
    "rxjs-tslint": "^0.1.3",
    "ts-loader": "^4.2.0",
    "ts-node": "^6.0.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.12.0",
    "typescript": "^2.8.3",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.2",
    "webpack-serve": "^0.3.2"
  },
  "peerDependencies": {
    "rxjs": ">=6.0.0 <7.0.0"
  }
}
