{
  "name": "rx-singleton-lock",
  "version": "1.1.2",
  "scripts": {
    "serve": "webpack-serve --config ./example/webpack.config.js --port 8080",
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write index.ts package.json webpack.config.js lib/**/* test/**/*",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "test": "npm run lint && npm run test:unit && npm run test:integration",
    "test:unit": "mocha --require ts-node/register 'test/unit/*.spec.ts'",
    "test:unit:watch": "npm run test:unit -- --watch --watch-extensions ts",
    "test:integration": "mocha --timeout 60000 --require ts-node/register 'test/integration/*.spec.ts'",
    "test:integration:watch": "npm run test:integration -- --watch --watch-extensions ts"
  },
  "description": "A small util that provides singleton locks for rxjs streams.",
  "license": "MIT",
  "files": [
    "dist/index.js",
    "dist/index.d.ts",
    "dist/lib"
  ],
  "main": "dist/index",
  "typings": "dist/index.d.ts",
  "keywords": [
    "rxjs",
    "lock",
    "mutex",
    "semaphore"
  ],
  "author": "Christoffer Skeppstedt",
  "repository": {
    "type": "git",
    "url": "https://github.com/cskeppstedt/rx-singleton-lock"
  },
  "homepage": "https://github.com/cskeppstedt/rx-singleton-lock",
  "bugs": "https://github.com/cskeppstedt/rx-singleton-lock",
  "contributors": [
    "Christoffer Skepptsedt (https://chsk.se)"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "@types/puppeteer": "^1.10.0",
    "@types/react": "^16.7.6",
    "@types/react-dom": "^16.0.9",
    "chai": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.2.0",
    "prettier": "^1.15.2",
    "puppeteer": "^1.10.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "rxjs": "^6.3.3",
    "rxjs-tslint": "^0.1.5",
    "ts-loader": "^5.3.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.16.0",
    "typescript": "^3.1.6",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2",
    "webpack-serve": "^2.0.2"
  },
  "peerDependencies": {
    "rxjs": ">=6.0.0 <7.0.0"
  }
}
